    "dev:open": "wait-on http://localhost:8080 && opener http://localhost:8080/",
    "dev:server": "webpack-dev-server --hot --inline",
    "postversion": "git push --follow-tags && conventional-github-releaser -p angular -r 0"
  },
  "devDependencies": {
    "abigail": "^1.7.1",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "codeclimate-test-reporter": "^0.3.1",
    "conventional-github-releaser": "^1.1.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "npm-statement": "^0.0.0",
    "opener": "^1.4.1",
    "pascal-case": "^1.1.2",
    "power-assert": "^1.4.1",
    "wait-on": "^1.4.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
