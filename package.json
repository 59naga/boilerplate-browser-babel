{
  "name": "your-module-name",
  "main": "lib",
  "files": [
    "lib",
    "release"
  ],
  "scripts": {
    "start": "karma start",
    "test": "karma start --single-run",
    "cover": "abby cover:*",
    "cover:test": "npm test",
    "cover:report": "npm-if TRAVIS \"codeclimate-test-reporter < coverage/lcov.info\"",
    "lint": "eslint src test",
    "build": "abby webpack --env",
